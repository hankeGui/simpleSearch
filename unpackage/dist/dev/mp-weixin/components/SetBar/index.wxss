.setbar{
	position: absolute;
	z-index: 10;
	bottom: 0;
	width: 100%;
	height: 480rpx;
	color: #333333;
	background-color: #FFFFFF;
	display: flex;
	flex-direction: column;
	padding: 20rpx 25rpx;
}
.set-opt{
	display: flex;
	flex-wrap: wrap;
	flex-direction: row;
	align-items: center;
	justify-content: space-between;
}
.opt{
	margin-top: 40rpx;
	width: 140rpx;
	text-align: center;
	font-size: 30px;
}
.opt-name{
	font-size: 14px;
	margin-top: 6rpx;
}
.set-close{
	width: 100%;
	margin-top: 50rpx;
	text-align: center;
	font-size: 24px;
}

